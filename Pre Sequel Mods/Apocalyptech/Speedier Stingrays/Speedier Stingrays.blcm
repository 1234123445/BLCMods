					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Class_StingRay_FlakBurst AfterburnerBoostTime 3.5</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Class_StingRay_FlakBurst AfterburnerImpulse.Z 32500</code>
				</hotfix>
			</category>
			<category name="Cryo Rocket Stingray">
				<hotfix name="ApocHotfix" package="GD_Co_StingRay_Streaming">
					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Vehicle_StingRay_CryoRocket MaxSpeed 3400</code>
