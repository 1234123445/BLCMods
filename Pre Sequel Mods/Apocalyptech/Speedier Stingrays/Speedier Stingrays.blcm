					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Class_StingRay_CryoRocket AfterburnerForceMagnitude 200</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Class_StingRay_CryoRocket AfterburnerBoostTime 3.5</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Archetype.Class_StingRay_CryoRocket AfterburnerImpulse.Z 32500</code>
				</hotfix>
			</category>
			<category name="Common Attributes">
				<hotfix name="ApocHotfix" package="GD_Co_StingRay_Streaming">
					<code profiles="default">set GD_Co_StingRay_Streaming.ResourcePools.AfterburnerPool_StingRay BaseMaxValue.BaseValueConstant 45</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.ResourcePools.AfterburnerPool_StingRay BaseOnIdleRegenerationRate 25</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.ResourcePools.AfterburnerPool_StingRay BaseOnIdleRegenerationDelay 1.2</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Handling.Handling_StingRay MaxThrustForce 1600</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Handling.Handling_StingRay MaxReverseForce 1600</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Handling.Handling_StingRay MaxStrafeForce 2000</code>
					<code profiles="default">set GD_Co_StingRay_Streaming.Handling.Handling_StingRay TurnTorqueMax 4000</code>
				</hotfix>
			</category>
		</category>
	</body>
</BLCMM>

